   <string notr="true">/* 主窗口样式 */
QWidget#widget {
  background-color: rgba(255, 255, 255, 0.2); /* 白色背景 */
  border-radius: 15px; /* 圆角 */
  background-image: url(:/img/bg__2.jpg); /* 背景图片 */
}

/* 通用按钮样式 */
QToolButton {
  border-style: none; /* 无边框 */
  color: rgb(0, 0, 0); /* 黑色字体 */
}

/* 关闭按钮样式 */
QToolButton#closeButton,
QToolButton#closeButton2 {
  color: rgb(0, 0, 0); /* 黑色字体 */
}

QToolButton#closeButton::hover,
QToolButton#closeButton2::hover {
  color: rgba(255, 255, 255, 0.2); /* 白色字体 */
  background-color: rgba(255, 56, 56, 150); /* 红色背景，半透明 */
}

QToolButton#closeButton::hover {
  border-top-right-radius: 15px; /* 圆角 */
}

/* 最小化按钮样式 */
QToolButton#minimizeButton {
  color: rgb(0, 0, 0); /* 黑色字体 */
}

QToolButton#minimizeButton::hover {
  background-color: rgba(203, 203, 203, 150); /* 灰色背景，半透明 */
}

/* 设置按钮样式 */
QToolButton#settingButton::hover {
  background-color: rgba(203, 203, 203, 150); /* 灰色背景，半透明 */
}

/* 图片按钮样式 */
QToolButton#imgbtn {
  border: 2px groove white; /* 凹槽边框 */
  border-radius: 10px; /* 圆角 */
  padding: 2px 4px; /* 内边距 */
}

/* 按钮样式 */
QToolButton#friendadding::hover {
  border: 1px solid rgba(76, 108, 179, 0.7); /* 蓝色边框 */
  border-radius: 10px; /* 圆角 */
  background-color: rgba(176, 196, 222, 0.5); /* 浅蓝色背景 */
}

/* 按钮样式 */
QToolButton#chatlist::hover {
  border: 1px solid rgba(76, 108, 179, 0.7); /* 蓝色边框 */
  border-radius: 10px; /* 圆角 */
  background-color: rgba(176, 196, 222, 0.5); /* 浅蓝色背景 */
}

/* 按钮样式 */
QToolButton#friendlist::hover {
  border: 1px solid rgba(76, 108, 179, 0.7); /* 蓝色边框 */
  border-radius: 10px; /* 圆角 */
  background-color: rgba(176, 196, 222, 0.5); /* 浅蓝色背景 */
}

/* 列表样式 */
QFrame#friendrequest {
  background-color: rgba(173, 216, 230, 0.5); /* 浅蓝色背景 */
  border: none; /* 无边框 */
  border-top: 1px solid rgba(76, 108, 179, 0.7); /* 顶部边框 */
  border-bottom-right-radius: 15px; /* 圆角 */
  border-bottom-left-radius: 15px; /* 圆角 */
}

/* 对话框和各个框架的样式 */
QFrame#dialog {
  border: none; /* 无边框 */
  border-top: 1px solid rgba(76, 108, 179, 0.7); /* 顶部边框 */
  border-left: 1px solid rgba(76, 108, 179, 0.7); /* 左边框 */
  border-bottom-right-radius: 15px; /* 圆角 */
}

QFrame#frame,
QFrame#frame1,
QFrame#frame2,
QFrame#frame3 {
  background-color: rgba(76, 108, 169, 0.1); /* 浅蓝色背景 */
  border: none; /* 无边框 */
  border-top: 1px solid rgba(76, 108, 179, 0.7); /* 顶部边框 */
  border-bottom: 1px solid rgba(76, 108, 179, 0.7); /* 底部边框 */
  border-bottom-left-radius: 15px; /* 圆角 */
}

QFrame#frame {
  background-color: rgba(76, 108, 169, 0.1); /* 更浅的背景颜色 */
}

QLabel#username{
    font-size:24px;
}

QLabel#userid{
    font-size:20px;
}

